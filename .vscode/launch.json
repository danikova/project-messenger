{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run Server",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/backend/src/server.js",
            "resolveSourceMapLocations": [
                "${workspaceFolder}/**",
                "!**/node_modules/**"
            ],
            "env": {
                "NODE_ENV": "development"
            }
        }
    ]
}
